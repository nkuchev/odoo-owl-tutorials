<templates xml:space="preserve">
    <t t-name="awesome_tshirt.AlertWidget" owl="1">
        <div t-if="warnings.length" class="alert alert-warning" role="alert">
            <ul class="m-0">
                <t t-foreach="warnings" t-as="warning" t-key="warning">
                    <li><t t-out="warning"/></li>
                </t>
            </ul>
        </div>
    </t>
</templates>
